{
	"compilerOptions": {
		"typeRoots": ["node_modules/@types", "typings"],
		"lib": ["ES2015.Iterable", "ES2019.Symbol", "ES2019.Array", "ES2019"],
		"module": "CommonJS",
		"target": "ES2018",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"removeComments": true,
		"noImplicitAny": true,
		"outDir": "dist"
	},
	"include": ["scr"],
	"exclude": ["node_modules", "typings", "scr/types"]
}
